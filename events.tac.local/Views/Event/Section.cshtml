@inherits Glass.Mapper.Sc.Web.Mvc.GlassView<events.tac.local.Models.IEventSection>



<div class="container">
    <div class="row">
        <h1>Event Categories</h1>
    </div>
    <div class="row">
        <ul>
            @foreach (var item in Model.Children)
            {
                <li>
                    <a href="@item.Url">
                        <h3>@item.Name Events</h3>
                    </a>
                </li>
            }
        </ul>
    </div>

</div>